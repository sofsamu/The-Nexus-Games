{
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/pages/$1"
      },
      {
        "src": "/JS/(.*)",
        "dest": "/JS/$1"
      },
      {
        "src": "/images/(.*)",
        "dest": "/images/$1"
      },
      {
        "src": "/scss/(.*)",
        "dest": "/scss/$1"
      }
    ]
  }
  